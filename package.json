{
  "name": "jsproject",
  "version": "0.0.1",
  "name": "frontproject-development-serviceapi",
  "version": "1.0.0",
  "description": "API service for frontend project deployment methods comparison",
  "main": "index.js",
  "private": true,
  "type": "module",
  "description": "Orange-themed React + Vite + TypeScript frontend with mobile-optimized design",
  "scripts": {
    "start": "node index.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "open": "start http://localhost:5173/",
    "preview:open": "vite preview --open",
    "open:live": "start https://hyukiody.github.io/frontproject-development-serviceApi/",
    "status:deploy": "start https://github.com/hyukiody/frontproject-development-serviceApi/actions/workflows/deploy.yml",
    "status:ci": "start https://github.com/hyukiody/frontproject-development-serviceApi/actions/workflows/ci.yml"
  },
  "keywords": [
    "deployment",
    "frontend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "i18next": "^25.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.2.0",
    "vitest": "^2.1.5"
  }
}
